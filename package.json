{
  "name": "node-commerce",
  "version": "1.0.0",
  "description": "A shopping cart and ecommerce platform using nodejs, express and mongodb",
  "main": "main.js",
  "scripts": {
    "start": "node .",
    "server": "node .",
    "server:watch": "nodemon .",
    "assets": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "assets:watch": "npm run assets -- --watch",
    "assets:prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "engines": {
    "node": "12.10.0"
  },
  "keywords": [
    "Express",
    "NodeJs",
    "Shopping Cart",
    "ECommerce",
    "Stripe",
    "Paypal",
    "MongoDB",
    "Shopping"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/benbarron/node-commerce.git"
  },
  "author": "Benjamin Barron",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "cluster": "^0.7.7",
    "colors": "^1.4.0",
    "connect-mongo": "^3.1.2",
    "express": "^4.17.1",
    "express-device": "^0.4.2",
    "express-edge": "^2.0.1",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.17.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "https": "^1.0.0",
    "laravel-mix": "^5.0.0",
    "mongoose": "^5.7.11",
    "nodemailer": "^6.4.16",
    "os": "^0.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "rimraf": "^3.0.0",
    "stripe": "^7.14.0",
    "uuid": "^3.3.3",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.4",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.23.6",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
